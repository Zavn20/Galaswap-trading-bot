{
  "name": "galaswap-trading-bot-server",
  "version": "1.0.0",
  "description": "Server-side implementation of GalaSwap Trading Bot",
  "main": "real-trading-server-optimized.js",
  "scripts": {
    "start": "node real-trading-server-optimized.js",
    "dev": "nodemon real-trading-server-optimized.js",
    "start-original": "node real-trading-server-clean.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "optimize": "node server-optimizations.js",
    "lint": "eslint *.js",
    "format": "prettier --write *.js *.html",
    "analyze": "node --inspect real-trading-server-clean.js"
  },
  "keywords": [
    "galaswap",
    "trading",
    "bot",
    "defi",
    "galachain",
    "optimization",
    "performance"
  ],
  "author": "GalaSwap Trading Bot",
  "license": "MIT",
  "dependencies": {
    "@gala-chain/gswap-sdk": "^0.0.7",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.2.0",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "optimization": {
    "memory_limit": "512MB",
    "cache_ttl": 30,
    "rate_limit": {
      "window_ms": 60000,
      "max_requests": 100
    }
  }
}
